string IonVersion::GetFullVersion(std::string module)
{
	string v = module;
	v += " (";
	v += "5";
	v += ".";
	v += "0";
	v += ".";
	v += "0";
	v += ")\n";
	v += "Build: ";
	v += "";
	v += " (";
	v += "";
	v += ")\nSource: ";
	v += "";
	v += " (";
	v += "a074b60-acc25d6";
	v += ")\n";
	return v;
}

string IonVersion::GetVersion()
{
	string v = "5";
	v += ".";
	v += "0";
   	return (v);
}



